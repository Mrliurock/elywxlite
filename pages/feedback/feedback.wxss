.feedback{
}
.fd-head{
  background: #fff;
  padding: 15px;
}
.fd-ta{
  padding:0 15px;
  background: #fff;
}
.fd-ta textarea{
  border: 1px solid #eee;
  width: 100%;
  padding: .5em;
  box-sizing: border-box;
}
.fd-text{
  padding: 5px 15px;
  color: #a4a2a3;
}
.fd-input{
  padding: 0 15px;
  margin-bottom: 10px;
}
.fd-input input{
  border: 1px solid #eee;
  padding: .5em;
}
.fd-img{
  padding: 0 15px;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.fd-img image{
  width: 20vw;
  height: 20vw;
  margin-bottom: 10px;
  margin-right:2vw;
  border-radius: 3px;
}
image:nth-child(4),image:nth-child(8){
  margin-right: 0;
}
.fd-img view{
  height: 20vw;
  width: 20vw;
  border: 1px dotted #ddd;
  border-radius: 3px;
  line-height: 20vw;
  text-align: center;
  color: #04a9a3;
  font-size: 30px;
  font-weight: bold;
  margin-bottom: 10px;
  margin-right:2vw;
}
.submit{
  padding: 0 15px;
}
.submit button{
  background: #04a9a3;
  color: #fff;
}
.submit .button-hover {
  background-color: rgba(4, 169, 163, 0.7);
}